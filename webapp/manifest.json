{
	"_version": "1.8.0",
	"sap.app": {
		"id": "inventory.Inventory",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "inventory.Inventory.view.login",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.tnt":{}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "inventory.Inventory.i18n.i18n"
				}
			},
			"data": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "data.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "inventory.Inventory.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Routelogin",
					"pattern": "Routelogin",
					"target": [
						"Targetlogin"
					]
				},
				{
					"name": "AdminPage",
					"pattern": "AdminPage/{obj}",
					"target": "AdminPage"
				},
				{
					"name": "EmployeePage",
					"pattern": "EmployeePage/{obj}",
					"target": "EmployeePage"
				},
				{
					"name": "",
					"pattern": "",
					"target": "Targetlogin"
				},
				{
					"name": "StatusPage",
					"pattern": "StatusPage/{obj}",
					"target": "StatusPage"
				},
				{
					"name": "issueStatics",
					"pattern": "issueStatics",
					"target": "issueStatics"
				},
				{
					"name": "notifications",
					"pattern": "Notifications",
					"target": "notifications"
				}
				,
				{
					"name": "emp_Notification",
					"pattern": "emp_Notification",
					"target": "emp_Notification"
				}
			],
			"targets": {
				"Targetlogin": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "login",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "inventory.Inventory.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"EmployeePage": {
					"viewType": "XML",
					"viewName": "EmployeePage",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "inventory.Inventory.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"AdminPage": {
					"viewType": "XML",
					"viewName": "AdminPage",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "inventory.Inventory.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"StatusPage": {
					"viewType": "XML",
					"viewName": "StatusPage",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "inventory.Inventory.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"issueStatics": {
					"viewName": "issueStatics",
					"viewType": "XML",
					"viewId": "statics"
				},
				"notifications": {
					"viewName": "notifications",
					"viewType": "XML",
					"viewId": "notifications"
				},
				"emp_Notification": {
					"viewType": "XML",
					"viewName": "emp_Notification"
				}
			}
		}
	}
}